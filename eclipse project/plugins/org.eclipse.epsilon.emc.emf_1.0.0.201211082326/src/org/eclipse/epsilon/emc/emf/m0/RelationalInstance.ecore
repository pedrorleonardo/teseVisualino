  </tables>
  <tables name="InvoiceItem">
    <columns name="invoiceId" type="INT"/>
    <columns name="productId" type="INT"/>
    <columns name="quantity" type="REAL"/>
  </tables>
  <foreignKeys name="CustomerInvoice" oneToMany="false">
