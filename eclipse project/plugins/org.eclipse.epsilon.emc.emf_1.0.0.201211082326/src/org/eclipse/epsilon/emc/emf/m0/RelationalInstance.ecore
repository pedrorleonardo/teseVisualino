  <tables name="Customer">
    <columns name="customerId" type="INT"/>
    <columns name="details" type="VARCHAR"/>
    <columns name="age" type="INT"/>
    <columns name="credit" type="REAL"/>
  </tables>
  <tables name="Invoice">
    <columns name="invoiceId" type="INT"/>
